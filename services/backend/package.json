  "scripts": {
    "build": "tsc -p . && tsconfig-replace-paths -p tsconfig.json && yarn generate:version",
    "start": "node build/src/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --files src/index.ts | pino-pretty --colorize",
    "migrate-dev": "yarn migrate && yarn seed && yarn dev",
    "lint": "eslint src",
    "test": "NODE_ENV=test ts-mocha --paths",
    "coverage": "NODE_ENV=test nyc ts-mocha",
    "db": "yarn sequelize",
    "migrate": "yarn sequelize db:migrate",
    "migrate:undo": "yarn sequelize db:migrate:undo",
    "seed": "yarn sequelize db:seed:all",
    "drop": "yarn sequelize db:seed:undo:all",
    "generate:disclaimer": "yarn --silent licenses generate-disclaimer > licenses.txt",
    "generate:version": "echo \"{\\\"commit\\\":\\\"$GIT_COMMIT\\\", \\\"version\\\":\\\"$GIT_VERSION\\\"}\" > version.json"
  },
